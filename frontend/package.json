{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:e2e": "next dev -p 3005",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "audit": "npm audit --audit-level=high",
    "test": "vitest run",
    "test:watch": "vitest",
    "e2e:server": "E2E_TEST=true NEXT_PUBLIC_E2E_TEST=true NEXT_PUBLIC_RESERVE_API_BASE_URL=http://127.0.0.1:3005/e2e npm run build && E2E_TEST=true NEXT_PUBLIC_E2E_TEST=true NEXT_PUBLIC_RESERVE_API_BASE_URL=http://127.0.0.1:3005/e2e npx next start -p 3005",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui"
  },
  "dependencies": {
    "@rainbow-me/rainbowkit": "^2.2.10",
    "@tanstack/react-query": "^5.90.21",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "viem": "^2.46.3",
    "wagmi": "^3.5.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "jsdom": "^26.1.0",
    "msw": "^2.11.2",
    "prettier": "^3.8.1",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "ignoreScripts": [
    "sharp",
    "unrs-resolver"
  ],
  "trustedDependencies": [
    "sharp",
    "unrs-resolver"
  ]
}
